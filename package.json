{
    "name": "react-redux-template",
    "description": "An example app using the MERN stack, with the main branch using React and Redux.",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": ">=10.14.2",
        "npm": ">=6.4.1",
        "yarn": ">=1.22.5"
    },
    "scripts": {
        "start": " cd server && node index.js",
        "startDev": "cd ./server && yarn server && cd ../client && yarn start",
        "postinstall": "(cd server && yarn install); (cd client && yarn install)",
        "heroku-postbuild": "cd client && npm install && npm run build"
        
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@upstatement/prettier-config": "^0.3.0"
    },
    "dependencies": {
        "@date-io/date-fns": "^2.13.1",
        "@mui/lab": "^5.0.0-alpha.68",
        "date-fns": "^2.28.0",
        "mongoose": "^6.2.0",
        "node-notifier": "^8.0.1",
        "object-path": "^0.11.5"
    }
}
